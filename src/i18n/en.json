{
  "$schema": "./schema.json",
  "locale": "en",
  "dateFormats": ["DD/MM/YYYY", "MM/YYYY", "YYYY", "MMM Do YYYY", "MMMM Do YYYY", "MMM YYYY", "MMMM YYYY"],
  "keywords": {
    "approximate": ["c.", "ca.", "ca", "circa", "approx", "near", "around", "about"],
    "uncertain": ["maybe", "possibly"],
    "interval": {
